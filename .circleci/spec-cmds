TEST_OUTPUT_NAME=unit bundle exec appraisal rake spec:units
TEST_OUTPUT_NAME=integration bundle exec appraisal rake spec:integrations
TEST_OUTPUT_NAME=features bundle exec rake spec:features[~framework:rails]
for rt in $(bundle exec appraisal list | grep rails); do TEST_OUTPUT_NAME=features bundle exec appraisal $rt rake spec:features[framework:rails]; done;
